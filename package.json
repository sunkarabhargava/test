  "scripts": {
    "envsubst": "envsub --syntax dollar-both ./env-configs/env-template.js public/env.js",
    "setLocalEnv": "env-cmd --fallback -f ./env-configs/.env npm run envsubst",
    "setLocalEnvDev": "env-cmd --fallback -f ./env-configs/.env-dev npm run envsubst",
    "setLocalEnvNoAuth": "env-cmd --fallback -f ./env-configs/.env-noauth npm run envsubst",
    "quality": "npm run lint && npm run prettier && npm run test",
    "start": "npm run setLocalEnv && react-scripts --openssl-legacy-provider start",
    "start:dev": "concurrently \"npm run setLocalEnv\" \"cross-env PORT=3000 react-scripts --openssl-legacy-provider start\"",
    "start:local": "concurrently \"node mock\" \"npm run setLocalEnv\" \"cross-env PORT=3000 BROWSER=none vite --force\" \"launch-luminate-portal\"",
    "start:local:legacy": "concurrently \"node mock\" \"npm run setLocalEnv\" \"cross-env PORT=3000 BROWSER=none react-scripts --openssl-legacy-provider start\" \"launch-luminate-portal\"",
    "start:localProd": "concurrently \"node mock\" \"npm run setLocalEnv\" \"serve -s build\" \"launch-luminate-portal\"",
    "build": "tsc && cross-env NODE_OPTIONS=--max-old-space-size=8172 vite build",
    "build:mfe": "tsc && cross-env NODE_OPTIONS=--max-old-space-size=8172 vite build --mode mfe",
    "build:legacy": "cross-env DISABLE_NEW_JSX_TRANSFORM=true react-scripts --openssl-legacy-provider --max_old_space_size=8172 build",
    "test-no-coverage": "react-scripts test",
    "eject": "react-scripts eject",
    "test": "npm run setLocalEnv && react-scripts test --coverage --watchAll=false --silent",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "prettier": "prettier --check src/**/*.ts src/**/*.tsx",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx} && prettier --write src/*.{js,jsx,ts,tsx}",
    "pre-commit": "concurrently \"npm run format\"  \"npm run lint\"",
    "clear-jest": "jest --clearCache",
    "test:e2e:chrome": "cross-env TZ=Australia/Adelaide TS_NODE_PROJECT=e2e/tsconfig.json wdio run e2e/configs/wdio.local.chrome.conf.ts",
    "test:e2e:saucelabs": "cross-env TZ=Australia/Adelaide TS_NODE_PROJECT=e2e/tsconfig.json wdio run e2e/configs/wdio.sauce.conf.ts",
    "test:e2e:saucelabs:availability": "cross-env TZ=Australia/Adelaide TS_NODE_PROJECT=e2e/tsconfig.json  wdio run  e2e/configs/wdio.availability.conf.ts -l debug"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 85,
        "functions": 85,
        "lines": 95
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@by/portal-bde-component-library)/)"
    ],
    "coveragePathIgnorePatterns": [
      "src/serviceWorker",
      "src/index.tsx",
      "src/models"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
